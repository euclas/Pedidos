Sub drawmonthname(iDate As Date)
    '
    '============== Dia
    '
    ' Me.lblMonth = MonthName(Month(iDate), True) & " " & Year(iDate)
    '
    ' Me.LblDay.Top = Me.PicCal.Top + 30
    '
    ' Me.LblDay.Top = ((Me.PicCal.Top + Me.PicCal.Height) - Me.LblDay.Height) - HeaderMonthH
    '
    Me.LblDay.Top = HeaderMonthH - (Me.LblDay.Height + lButSOTop)
    '
    ' Me.LblDay.Left = Me.PicCal.Left + lButRight + 260
    '
    Me.LblDay.Left = Me.PicCal.Left + lButSORight + 400 ' 360
    '
    ' Me.LblDay.Width = upButLeft - lButRight ' - 60
    '
    Me.LblDay.Width = Me.PicCal.Width - (3 * ((lButSORight - lButSOLeft) + (rButRight - rButLeft))) - 100
    '
    Me.LblDay.Caption = Day(iDate)
    '
    Me.LblDay.Refresh
    '
    '============== Mes
    '
    ' Me.lblMonth.Top = Me.LblDay.Top + Me.LblDay.Height + 60
    '
    Me.LblMonth.Top = Me.LblDay.Top + Me.LblDay.Height + 40
    '
    ' Me.lblMonth.Left = Me.PicCal.Left + lButRight + 300
    '
    Me.LblMonth.Left = Me.PicCal.Left + lButSORight + 400 ' 360
    '
    ' Me.lblMonth.Width = upButLeft - lButRight ' - 60
    '
    Me.LblMonth.Width = Me.PicCal.Width - (3 * ((lButSORight - lButSOLeft) + (rButRight - rButLeft))) - 100
    '
    Me.LblMonth.Caption = lstMonthNames.List(Month(iDate) - 1)
    '
    '============== Ano
    '
    ' Me.LblYear.Top = Me.lblMonth.Top + Me.lblMonth.Height + 60
    '
    Me.LblYear.Top = Me.LblMonth.Top + Me.LblMonth.Height + 40
    '
    ' Me.LblYear.Left = Me.PicCal.Left + lButRight + 300
    '
    Me.LblYear.Left = Me.PicCal.Left + lButSORight + 400 ' 360
    '
    ' Me.LblYear.Width = upButLeft - lButRight ' - 60
    '
    Me.LblYear.Width = Me.PicCal.Width - (3 * ((lButSORight - lButSOLeft) + (rButRight - rButLeft))) - 100
    '
    Me.LblYear.Caption = Year(iDate)
    '
End Sub
