Sub FillArrayMonth(iDate As Date)
    '
    Dim mMonth As Integer, nMonthDays As Integer, nFirstDay As Integer
    Dim nArrayElCnt As Integer
    Dim tmpDate As Date
    Dim ntxtWidth As Integer
    Dim I As Integer
    '
    mMonth = Month(iDate)
    tmpDate = DateAdd("m", 1, DateSerial(Year(iDate), Month(iDate), 1))
    nMonthDays = Day(DateAdd("d", -1, tmpDate))
    nFirstDay = Weekday(DateSerial(Year(iDate), Month(iDate), 1), vbUseSystem)
    '
    Me.PicCal.ForeColor = vbBlack
    '
    For I = 0 To nFirstDay - 1
        aDateArray(I) = Empty
    Next I
    '
    For I = nFirstDay + nMonthDays To UBound(aDateArray)
        aDateArray(I) = Empty
    Next I
    I = 1
    '
    For nArrayElCnt = nFirstDay To nFirstDay + nMonthDays - 1
        '
        aDateArray(nArrayElCnt) = I
        '
        ' Define a cor dos dias do calendário
        '
        ' < 270
        '
        If (((nArrayElCnt - 1) Mod 7) * DayCellWidth) + aDayWidth(I) < DayCellWidth Then
           '
           ' If MsgBox("X = " & CInt((((nArrayElCnt - 1) Mod 7) * DayCellWidth) + aDayWidth(i)), vbYesNo + vbQuestion, App.Title) = vbNo Then Exit Sub
           '
           Me.PicCal.ForeColor = vbRed
           '
        Else
           '
           Me.PicCal.ForeColor = vbBlack
           '
        End If
        '
        PicCal.DrawText aDateArray(nArrayElCnt), (((nArrayElCnt - 1) Mod 7) * DayCellWidth) + aDayWidth(I), (HeaderMonthH + HeaderDaysH + ((nArrayElCnt - 1) \ 7) * DayCellHeight)
        '
        I = I + 1
        '
    Next nArrayElCnt
    '
    ' paint input date in reverse color
    '
    nArrayElCnt = nFirstDay + Day(iDate) - 1
    '
    DrawReverseDate (nArrayElCnt) Mod 7, (nArrayElCnt) \ 7 + 1, prevX, prevY, nArrayElCnt
    '
End Sub
