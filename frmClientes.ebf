Private Sub TimerYear_Timer()
    '
    If ButtonDirection > 0 And PicCal.Visible = True Then
       '
       Select Case ButtonDirection
       
       Case ButtonDirectionLeft
            '
            stDate = DateAdd("m", -1, stDate)
            '
            If Year(stDate) < mAnoInicial Then stDate = DateAdd("yyyy", 1, stDate)
            '
            drawmonthname stDate
            '
       Case ButtonDirectionRight
            '
            stDate = DateAdd("m", 1, stDate)
            '
            If Year(stDate) > mAnoFinal Then stDate = DateAdd("yyyy", -1, stDate)
            '
            drawmonthname stDate
            '
       Case ButtonDirectionUp
            '
            stDate = DateAdd("yyyy", 1, stDate)
            '
            If Year(stDate) > mAnoFinal Then stDate = DateAdd("yyyy", -1, stDate)
            '
            drawmonthname stDate
            '
       Case ButtonDirectionDn
            '
            stDate = DateAdd("yyyy", -1, stDate)
            '
            If Year(stDate) < mAnoInicial Then stDate = DateAdd("yyyy", 1, stDate)
            '
            drawmonthname stDate
            '
       End Select
       '
    End If
    '
End Sub
