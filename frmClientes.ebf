Sub OpenCloseCalendar(strValidaData As String, ctrlContainer As Control)
    '
    Dim I As Integer
    '
    If Me.PicCal.Visible = False Then
       '
       Me.PicCal.Visible = True
       '
       If (Trim(strValidaData) = "") Or (Trim(strValidaData) = "**/**/****") Or (Trim(strValidaData) = "//") Then
          '
          strValidaData = Right("00" & Trim(CStr(Day(Now))), 2) & "/" & Right("00" & Trim(CStr(Month(Now))), 2) & "/" & Right("0000" & Trim(CStr(Year(Now))), 4)
          '
          stDate = strValidaData
          '
          mNula = True
          '
       Else
          '
          stDate = strValidaData
          '
          mNula = False
          '
       End If
       '
       SetPicCalPos ctrlContainer
       '
       prevX = 0
       prevY = 0
       '
       Me.PicCal.FillColor = vbWhite
       '
       '== Desenha o quadro quando entra no calendario.
       '
       Me.PicCal.DrawLine 0, HeaderDaysH + HeaderMonthH, PicCal.ScaleWidth, PicCal.ScaleHeight, vbBlack, True, False
       '
       Me.LblDay.Visible = True
       Me.LblMonth.Visible = True
       Me.LblYear.Visible = True
       '
       Me.PicCal.ZOrder vbBringToFront ' vbSendToBack
       '
       Me.LblDay.ZOrder vbBringToFront ' vbSendToBack
       Me.LblMonth.ZOrder vbBringToFront ' vbSendToBack
       Me.LblYear.ZOrder vbBringToFront ' vbSendToBack
       '
       FillArrayMonth stDate
       '
       '============ =============== ================
       '
       ' Desenha não sei onde...
       '
       Me.PicCal.DrawLine 0, 0, PicCal.ScaleWidth, PicCal.ScaleHeight, vbBlack, True, False
       '
       Me.PicCal.DrawPicture strPrograma & "\mscroll.bmp", 10, 10
       '
       FillArrayMonth stDate
       '
       '========================================
       ' Escreve a inicial dos dias da semana
       '========================================
       '
       Me.PicCal.FontBold = True
       '
       For I = 1 To 7
           '
           '========================================
           ' Me.piccal.DrawText Mid(WeekdayName(i, True, vbUseSystem), 1, 1), ((DayCellWidth - piccal.TextWidth(CStr(Mid(WeekdayName(i, True, vbUseSystem), 1, 1))) * Screen.TwipsPerPixelX) \ 2 + (i - 1) * DayCellWidth) + 15, HeaderMonthH
           '========================================
           '
           If I = 1 Then
              '
              Me.PicCal.ForeColor = vbRed
              '
           Else
              '
              Me.PicCal.ForeColor = vbBlue
              '
           End If
           '
           Me.PicCal.DrawText Mid(List1.List(I), 1, 1), ((DayCellWidth - PicCal.TextWidth(CStr(Mid(List1.List(I), 1, 1))) * Screen.TwipsPerPixelX) \ 2 + (I - 1) * DayCellWidth) + 15, HeaderMonthH + HeaderDaysH - DayHeaderTop
           '
       Next I
       '
       Me.PicCal.DrawLine 0, HeaderMonthH + HeaderDaysH, PicCal.ScaleWidth, HeaderMonthH + HeaderDaysH, vbBlack, False, False
       '
       Me.PicCal.FontBold = False
       '
       '============ =============== ================
       '
       drawmonthname stDate
       '
       Me.PicCal.SetFocus
       '
    Else
       '
       mValida = 0
       '
       Me.LblDay.Visible = False
       Me.LblMonth.Visible = False
       Me.LblYear.Visible = False
       '
       Me.PicCal.Visible = False
       '
       If Me.Controls(CallingControl).Text = "" Or Me.Controls(CallingControl).Text = "**/**/****" Or Me.Controls(CallingControl).Text = "//" Then
          '
          Me.Controls(CallingControl).Text = "**/**/****"
          '
       End If
       '
    End If
    '
End Sub
