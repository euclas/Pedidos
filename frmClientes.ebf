Sub DrawReverseDate(xC As Integer, yC As Integer, xP As Integer, yP As Integer, arrayDay As Integer)
    '
    If arrayDay Mod 7 <> 0 Then
       '
       Me.PicCal.DrawLine ((xC - 1) * DayCellWidth) + 10 + 15, ((yC - 1) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, ((xC) * DayCellWidth) - 10 + 15, ((yC) * DayCellHeight) + HeaderMonthH + HeaderDaysH - 10, vbRed, True, True
       '
       Me.PicCal.ForeColor = vbWhite
       '
       PicCal.DrawText aDateArray(arrayDay), (((arrayDay - 1) Mod 7) * DayCellWidth) + aDayWidth(aDateArray(arrayDay)) + 15, HeaderMonthH + HeaderDaysH + ((arrayDay - 1) \ 7) * DayCellHeight
       '
    Else
       '
       If ((yC - 2) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10 > (HeaderMonthH + HeaderDaysH) Then
          '
          Me.PicCal.DrawLine ((6) * DayCellWidth) + 10 + 15, ((yC - 2) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, ((7) * DayCellWidth) - 10 + 15, ((yC - 1) * DayCellHeight) + HeaderMonthH + HeaderDaysH - 10, vbRed, True, True
          '
          Me.PicCal.ForeColor = vbWhite
          '
          PicCal.DrawText aDateArray(arrayDay), ((6) * DayCellWidth) + aDayWidth(aDateArray(arrayDay)) + 15, HeaderMonthH + HeaderDaysH + ((arrayDay - 2) \ 7) * DayCellHeight
          '
       End If
       '
    End If
    '
    xP = xC
    yP = yC
    '
End Sub
