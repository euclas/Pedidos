Private Sub picCal_MouseDown(ByVal Button As Long, ByVal Shift As Long, ByVal X As Double, ByVal Y As Double)
   '
   Dim xCell As Integer
   Dim yCell As Integer
   Dim iDay As Integer
   Dim strData As String
   '
   ' Check for click on the SO Date button
    '
    If X > lButSOLeft And X < lButSORight And Y > lButSOTop And Y < lButSOBottom Then
       '
       stDate = Now
       '
       Me.PicCal.DrawLine 0, HeaderMonthH + HeaderDaysH, PicCal.ScaleWidth, PicCal.ScaleHeight, vbBlack, True, False
       '
       FillArrayMonth stDate
       '
       drawmonthname stDate
       '
       Exit Sub
       '
    End If
    '
    ' Check for click on the Undefinided Date button
    '
    If X > rButDtLeft And X < rButDtRight And Y > rButDtTop And Y < rButDtBottom Then
       '
       FillArrayMonth stDate
       '
       'DrawReverseDate xCell, yCell, prevX, prevY, iDay
       '
       Me.Controls(CallingControl).Text = "**/**/****"
       '
       PicCal.Visible = False
       '
       lstMonthNames.Visible = False
       '
       Me.LblDay.Visible = False
       Me.LblMonth.Visible = False
       Me.LblYear.Visible = False
       '
       ' Exit Sub
       '
    End If
    '
    'check for click on the left button = Atraza um mes
    '
    Me.PicCal.FillColor = vbWhite
    '
    If X > lButLeft And X < lButRight And Y > lButTop And Y < lButBottom Then
        '
        Me.TimerYear.Interval = 500
        '
        ButtonDirection = ButtonDirectionLeft
        '
        stDate = DateAdd("m", -1, stDate)
        '
        Me.PicCal.DrawLine 0, HeaderMonthH + HeaderDaysH, PicCal.ScaleWidth, PicCal.ScaleHeight, vbBlack, True, False
        '
        FillArrayMonth stDate
        '
        drawmonthname stDate
        '
        Exit Sub
    End If
    '
    'check for click on the right button = Adianta um mes
    '
    If X > rButLeft And X < rButRight And Y > rButTop And Y < rButBottom Then
        '
        Me.TimerYear.Interval = 500
        '
        ButtonDirection = ButtonDirectionRight
        '
        stDate = DateAdd("m", 1, stDate)
        '
        Me.PicCal.DrawLine 0, HeaderMonthH + HeaderDaysH, PicCal.ScaleWidth, PicCal.ScaleHeight, vbBlack, True, False
        '
        FillArrayMonth stDate
        '
        drawmonthname stDate
        '
        Exit Sub
    End If
    '
    'check for click on scroll year up = Atraza um Ano
    '
    If X > upButLeft And X < upButRight And Y > upButTop And Y < upButBottom Then
       '
       ButtonDirection = ButtonDirectionUp
       '
       Me.TimerYear.Interval = 50
       '
       Exit Sub
    End If
    '
    'check for click on scroll year down = Adianta um Ano
    '
    If X > dnButLeft And X < dnButRight And Y > dnButTop And Y < dnButBottom Then
        '
        ButtonDirection = ButtonDirectionDn
        '
        Me.TimerYear.Interval = 50
        '
        Exit Sub
    End If
    '
    'check for click in the calendar itself = Carrega o dia
    '
    If Y > HeaderMonthH + HeaderDaysH Then
       '
       ' iDay = (yCell - 1) * 7 + xCell
       ' If Not IsEmpty(aDateArray(iDay)) Then
       '
       If prevX <> 0 And prevY <> 0 Then
          '
          iDay = (prevY - 1) * 7 + prevX
          '
          If iDay Mod 7 <> 0 Then
             '
             '=== Se for Tap em célula em branco (Fora dos Dias possíveis) Marca o dia corrente no Grid que vai ser transportado para o campocom a cor amarela
             '
             Me.PicCal.DrawLine ((prevX - 1) * DayCellWidth) + 10 + 15, ((prevY - 1) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, ((prevX) * DayCellWidth) - 10 + 15, ((prevY) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, vbYellow, True, True
             '
             Me.PicCal.ForeColor = vbBlack
             '
             Me.PicCal.DrawText aDateArray(iDay), (((iDay - 1) Mod 7) * DayCellWidth) + aDayWidth(aDateArray(iDay)) + 15, HeaderMonthH + HeaderDaysH + ((iDay - 1) \ 7) * DayCellHeight
             '
          Else
             '
             '=== Marca o dia corrente no Grid que vai ser transportado para o campo
             '
               Me.PicCal.DrawLine ((6) * DayCellWidth) + 10 + 15, ((prevY - 1) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, ((7) * DayCellWidth) - 10 + 15, ((prevY - 1) * DayCellHeight) + HeaderMonthH + HeaderDaysH + 10, vbRed, True, True
             '
             Me.PicCal.ForeColor = vbBlack
             '
             Me.PicCal.DrawText aDateArray(iDay), ((6) * DayCellWidth) + aDayWidth(aDateArray(iDay)) + 15, HeaderMonthH + HeaderDaysH + ((iDay - 2) \ 7) * DayCellHeight
             '
          End If
          '
       End If
       '
       xCell = ((X - 15) \ DayCellWidth) + 1
       yCell = Fix((Y - (HeaderMonthH + HeaderDaysH)) \ DayCellHeight) + 1
       '
       '      If y > HeaderMonthH + HeaderDaysH Then
       '
       iDay = (yCell - 1) * 7 + xCell
       '
       If Not IsEmpty(aDateArray(iDay)) Then
          '
          '
          '
          '
          If Me.TimerYear.Interval > 0 Then
             '
             ButtonDirection = 0
             '
             Me.TimerYear.Interval = 0
             '
          End If
          '
          '
          '
          '
          DrawReverseDate xCell, yCell, prevX, prevY, iDay
          '
          strData = Right("00" & Trim(CStr(aDateArray(iDay))), 2) & "/" & Right("00" & Trim(CStr(Month(stDate))), 2) & "/" & Right("0000" & Trim(CStr(Year(stDate))), 4)
          '
          ' FormatDateTime(DateSerial(Year(stDate), Month(stDate), aDateArray(iDay)), vbShortDate)
          '
          Me.Controls(CallingControl).Text = strData
          '
          PicCal.Visible = False
          '
          lstMonthNames.Visible = False
          '
          Me.LblDay.Visible = False
          Me.LblMonth.Visible = False
          Me.LblYear.Visible = False
          '
       End If
       '
    End If
    '
End Sub
